{
    "name": "mathewekuwam/jetbar",
    "description": "It adds a sidebar dashboard theme for Laravel applications with Jetstream.",
    "keywords": [
        "laravel",
        "jetstream",
        "sidebar",
        "jetbar"
    ],
    "homepage": "https://github.com/mathewekuwam/jetbar",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Ariel Mejia Dev",
            "email": "arielmejiadev@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.3|^8.0|^8.1|^8.2|^8.3",
        "illuminate/filesystem": "^8.0|^9.0|^10.0",
        "illuminate/support": "^8.0|^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "Arielmejiadev\\Jetbar\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Arielmejiadev\\Jetbar\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Arielmejiadev\\Jetbar\\JetbarServiceProvider"
            ],
            "aliases": {
                "Jetbar": "Arielmejiadev\\Jetbar\\JetbarFacade"
            }
        }
    }
}
